        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainPart.vMainScreen"></activity>
        <activity android:name=".MainPart.QuestionsActivity" />
        <activity android:name=".ChatActivity.ChatAcvitity" />
